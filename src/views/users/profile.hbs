<main class="index">
    <div class="container p-4">
    <div class="row">
        <div class="col-md-8 mx-auto">
            <div class="card text-center">
                <div class="card-body">
                    <h3>Bienvenido {{user.fullname}}</h3>
                    <h4>Usuario: {{user.username}}</h4>
                    <div>
                         {{#if (eq user.typeUser "EXPONENT")}}<a href="/projects" class="btn btn-primary m-4">Tus Proyectos</a>
                         {{/if}}
                         {{#if (eq user.typeUser "ADMIN")}}<a href="/signupAdmin" class="btn btn-primary m-4">Registrar usuarios</a>
                         {{/if}}
                        <a href="/updateProfile" class="btn btn-success m-4">Editar</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</main>